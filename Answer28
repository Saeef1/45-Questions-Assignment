let person : number = 1;
if (person<2) {
    console.log("the person is a baby")
} 
else if (person<=4) {
    console.log("the person is a toddler")
}
else if (person<=13) {
    console.log("the person is a kid")
}
else if (person<=18) {
    console.log("the person is a teenager")
}
else if (person<=65) {
    console.log("the person is a adult")
}
else {
    console.log("the person is elder")    
}
