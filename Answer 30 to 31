//Exercise 30
let username: string[] = ["Hammad", "Yasir", "Jamal", "Admin", "Markup"];
if(username.length > 0){ username.forEach(
    username => {
        if (username.includes("Admin")) {
            console.log(`Hello Admin, would you like to see a status report?`)
        } else {
            `such as Hello ${username}, thank you for logging in again.`
        }});
    }
else{
        console.log("we need to find more users")
    };
